{
    "name": "Create user",
    "request": {
        "method": "POST",
        "path": "/v1/users",
        "body": {
            "email": "my@email.com",
            "password": "my-secret-password"
        },
        "useToken": false
    },
    "response": {
        "status": 200,
        "body": {
            "email": "my@email.com",
            "role": "USER"
        }
    },
    "setEnv": [
        {
            "envKey": "userId",
            "responseKey": "id"
        }
    ]
}