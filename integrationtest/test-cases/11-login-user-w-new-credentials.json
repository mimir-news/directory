{
    "name": "Login user w new credentials",
    "request": {
        "method": "POST",
        "path": "/v1/login",
        "body": {
            "email": "my-new@email.com",
            "password": "my-new-password"
        },
        "useToken": false
    },
    "response": {
        "status": 200
    },
    "setEnv": [
        {
            "envKey": "authToken",
            "responseKey": "token"
        }
    ]
}